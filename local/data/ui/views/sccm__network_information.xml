      <chart>
        <title>Manufacturer</title>
        <search>
          <query>index=sccm sourcetype=sccm-netcard | dedup ComputerName AdapterType00 ProductName00 ServiceName00 Manufacturer00 | fillnull value="-"|  stats count by Manufacturer00</query>
          <earliest>0</earliest>
          <latest></latest>
